@model IEnumerable<Portfolio.Models.ProjectSkill>

@{
    ViewBag.Title = "Index";
}
<body id="body4">
    <h2>Index</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProjectSkillLevel)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Project.ProjectName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skill.SkillName)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProjectSkillLevel)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Project.ProjectName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Skill.SkillName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.ProjectSkillID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.ProjectSkillID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.ProjectSkillID })
                </td>
            </tr>
        }

    </table>
</body>